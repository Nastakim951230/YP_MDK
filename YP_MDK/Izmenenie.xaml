<Window x:Class="YP_MDK.Izmenenie"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:YP_MDK"
        mc:Ignorable="d"
        Title="Изменить данные" Height="250" Width="500">
    <Grid>
        <StackPanel>
            <GroupBox Header="Изменить статус">
                <ComboBox Name="statusCB"></ComboBox>
            </GroupBox>
            <GroupBox Header="Изменить дату доставки">
                <DatePicker Name="date"></DatePicker>
            </GroupBox>
            <StackPanel Orientation="Horizontal">
                <Button Name="Nazad" Click="Nazad_Click">Назад</Button>
                <Button Name="Sohran" Click="Sohran_Click">Сохранить </Button>
            </StackPanel>
        </StackPanel>
    </Grid>
</Window>
